// Simple Sparkline Component for Network Traffic\
const Sparkline = (\{ data, color = "#a855f7" \}: \{ data: number[], color?: string \}) => \{\
  const max = Math.max(...data, 10);\
  const min = 0;\
  const height = 40;\
  const width = 120;\
  \
  const points = data.map((val, i) => \{\
    const x = (i / (data.length - 1)) * width;\
    const y = height - ((val - min) / (max - min)) * height;\
    return `$\{x\},$\{y\}`;\
  \}).join(' ');\
\
  return (\
    <svg width="100%" height=\{height\} viewBox=\{`0 0 $\{width\} $\{height\}`\} className="overflow-visible filter drop-shadow-md">\
      <defs>\
        <linearGradient id=\{`gradient-$\{color\}`\} x1="0" x2="0" y1="0" y2="1">\
          <stop offset="0%" stopColor=\{color\} stopOpacity="0.5" />\
          <stop offset="100%" stopColor=\{color\} stopOpacity="0" />\
        </linearGradient>\
      </defs>\
      <polyline\
        fill="none"\
        stroke=\{color\}\
        strokeWidth="2"\
        points=\{points\}\
        strokeLinecap="round"\
        strokeLinejoin="round"\
      />\
      <path \
        d=\{`M 0 $\{height\} L $\{points\} L $\{width\} $\{height\} Z`\} \
        fill=\{`url(#gradient-$\{color\})`\}\
        stroke="none" \
      />\
    </svg>\
  );\
\};\
